<?xml version="1.0" encoding="UTF-8"?>
<tool id="opennlp.coreference" name="OpenNLP Coreference" version="1.0.0">
  <description>adds OpenNLP coreference annotations</description>
  <command interpreter="lsd">invoke.lsd Coreference $input $output</command>
  <inputs>
    <param name="input" type="data" format="lif" label="Input"/>
  </inputs>
  <outputs>
    <data name="output" format="lif"/>
  </outputs>
  <help>Requires tokenized text.</help>
</tool>
